{
  "name": "coffee-shop",
  "description": "Baker 75 Street personal project",
  "version": "0.1.0",
  "private": true,
  "autor": "Peter Kulko <petrokulko@gmail.com>",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "bundle-analyzer": "craco build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "lint": "npm run stylelint && eslint ./src --ext .js,.jsx"
  },
  "dependencies": {
    "@edx/stylelint-config-edx": "^2.3.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "animate.css": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "sass": "^1.63.6",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "formik": "^2.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.4",
    "react-hook-form": "^7.44.3",
    "react-redux": "^8.0.7",
    "react-test-renderer": "^18.2.0",
    "scss-reset": "^1.4.2",
    "stylelint": "^15.2.0",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "lint": {
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint",
      "git add --force"
    ],
    "!coverage/**/*": "eslint --ignore-pattern coverage"
  }
}
